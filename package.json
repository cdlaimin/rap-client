{
  "name": "rap-client",
  "version": "2.0.0",
  "description": "rap-client react app",
  "main": "app.js",
  "scripts": {
    "start": "yarn dev",
    "dev": "vite --port 3001",
    "build": "vite build",
    "test": "npm run lint",
    "lint": "echo \"lint checking...\" && eslint . --ext .ts,.tsx --fix && echo \"tsc checking...\"",
    "lint-fix": "echo \"lint checking...\" && eslint . --ext .ts,.tsx --fix && echo \"tsc checking...\"",
    "build-docker": "docker build --rm -f \"Dockerfile\" -t udfteam/rap-client:latest ."
  },
  "resolutions": {
    "react-error-overlay": "6.0.9",
    "react-floater": "0.8.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:infra-fe/rap-client.git"
  },
  "author": "Libby, Bosn",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.3",
    "@mui/styles": "^5.6.1",
    "animate.css": "4.1.1",
    "awesome-debounce-promise": "^2.1.0",
    "axios": "^0.24.0",
    "chart.js": "^3.6.0",
    "classnames": "^2.3.1",
    "clipboard-copy": "^4.0.1",
    "connected-react-router": "^6.9.2",
    "debounce-promise": "^3.1.2",
    "formik": "^2.2.9",
    "formik-mui": "^4.0.0-alpha.3",
    "history": "^4.10.1",
    "i18next": "^21.3.3",
    "i18next-browser-languagedetector": "^6.1.2",
    "json5": "^2.2.0",
    "jsoneditor": "^9.7.4",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "markdown-to-jsx": "^7.1.3",
    "material-ui-popup-state": "^4.0.1",
    "mockjs": "1.1.0",
    "moment": "^2.29.1",
    "monaco-editor": "^0.34.0",
    "notistack": "^2.0.3",
    "nprogress": "0.2.0",
    "prop-types": "15.7.2",
    "rap-import-utils": "^0.0.3",
    "rc-tooltip": "^5.1.1",
    "react": "^17.0.2",
    "react-custom-scroll": "^4.3.0",
    "react-dom": "^17.0.2",
    "react-hotkeys": "^2.0.0",
    "react-i18next": "^11.16.6",
    "react-icons": "4.3.1",
    "react-joyride": "^2.5.1",
    "react-modal": "3.14.3",
    "react-monaco-editor": "^0.50.1",
    "react-redux": "^7.2.8",
    "react-router": "5.2.1",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.3.0",
    "redux": "4.1.2",
    "redux-api-middleware": "^3.2.1",
    "redux-saga": "1.1.3",
    "reselect": "^4.1.1",
    "sortablejs": "1.14.0",
    "urijs": ">=1.19.7",
    "yup": "^0.32.11"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "fetch"
    ],
    "ignore": [
      "/build"
    ]
  },
  "devDependencies": {
    "@types/chart.js": "^2.9.34",
    "@types/classnames": "^2.3.0",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.2",
    "@types/json5": "^0.0.30",
    "@types/lodash": "^4.14.176",
    "@types/mockjs": "^1.0.6",
    "@types/nprogress": "^0.2.0",
    "@types/rc-tooltip": "^3.7.5",
    "@types/react": "^18.0.4",
    "@types/react-dom": "^18.0.0",
    "@types/react-modal": "^3.13.1",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.3.2",
    "@types/sortablejs": "^1.10.7",
    "@types/urijs": "^1.19.17",
    "@types/yup": "^0.29.13",
    "@vitejs/plugin-react": "^2.0.0",
    "npm-run-all": "4.1.5",
    "pre-commit": "^1.2.2",
    "react-scripts": "^4.0.3",
    "rollup-plugin-visualizer": "^5.7.1",
    "sass": "^1.43.4",
    "standard": "16.0.4",
    "typescript": "^4.5.5",
    "vite": "^3.0.4",
    "vite-plugin-imp": "^2.2.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/adjacent-overload-signatures": "error",
          "@typescript-eslint/array-type": [
            "error",
            {
              "default": "array-simple"
            }
          ],
          "@typescript-eslint/ban-types": "error",
          "@typescript-eslint/consistent-type-assertions": "error",
          "@typescript-eslint/consistent-type-definitions": "off",
          "@typescript-eslint/explicit-member-accessibility": [
            "off",
            {
              "accessibility": "explicit"
            }
          ],
          "@typescript-eslint/indent": [
            "error",
            2,
            {
              "FunctionDeclaration": {
                "parameters": "first"
              },
              "FunctionExpression": {
                "parameters": "first"
              },
              "SwitchCase": 1,
              "MemberExpression": 1
            }
          ],
          "@typescript-eslint/interface-name-prefix": "off",
          "@typescript-eslint/member-delimiter-style": [
            "error",
            {
              "multiline": {
                "delimiter": "none",
                "requireLast": true
              },
              "singleline": {
                "delimiter": "semi",
                "requireLast": false
              }
            }
          ],
          "@typescript-eslint/member-ordering": "off",
          "@typescript-eslint/no-empty-function": "error",
          "@typescript-eslint/no-empty-interface": "off",
          "@typescript-eslint/no-explicit-any": "off",
          "@typescript-eslint/no-inferrable-types": [
            "error",
            {
              "ignoreParameters": true,
              "ignoreProperties": true
            }
          ],
          "@typescript-eslint/no-misused-new": "error",
          "@typescript-eslint/no-namespace": "error",
          "@typescript-eslint/no-parameter-properties": "off",
          "@typescript-eslint/no-redeclare": "off",
          "@typescript-eslint/no-require-imports": "off",
          "@typescript-eslint/no-this-alias": "off",
          "@typescript-eslint/no-use-before-declare": "off",
          "@typescript-eslint/no-var-requires": "off",
          "@typescript-eslint/prefer-for-of": "off",
          "@typescript-eslint/prefer-function-type": "off",
          "@typescript-eslint/prefer-namespace-keyword": "error",
          "@typescript-eslint/quotes": [
            "error",
            "single",
            {
              "allowTemplateLiterals": true
            }
          ],
          "@typescript-eslint/semi": [
            "error",
            "never"
          ],
          "@typescript-eslint/triple-slash-reference": "error",
          "@typescript-eslint/type-annotation-spacing": "error",
          "@typescript-eslint/unified-signatures": "error",
          "arrow-body-style": "off",
          "arrow-parens": [
            "off",
            "as-needed"
          ],
          "camelcase": "off",
          "capitalized-comments": "off",
          "complexity": "off",
          "constructor-super": "error",
          "curly": "error",
          "dot-notation": "off",
          "eol-last": "error",
          "eqeqeq": [
            "error",
            "smart"
          ],
          "guard-for-in": "error",
          "id-blacklist": "off",
          "id-match": [
            "error",
            "^(?!(any|Number|number|String|string|Boolean|boolean|Undefined|undefined)$).*$",
            {
              "onlyDeclarations": true
            }
          ],
          "import/order": "off",
          "import/no-mutable-exports": "off",
          "import/no-unused-modules": "off",
          "import/no-cycle": "off",
          "import/no-amd": "error",
          "max-classes-per-file": "off",
          "max-len": [
            "error",
            {
              "ignorePattern": "^import |^export \\{(.*?)\\}|^[ ]*export const |`.*`|\".*\"",
              "code": 150
            }
          ],
          "new-parens": "error",
          "no-bitwise": "error",
          "no-caller": "error",
          "no-cond-assign": "error",
          "no-console": "error",
          "no-debugger": "error",
          "no-empty": "error",
          "no-eval": "error",
          "no-fallthrough": "off",
          "no-import-side-effect": "off",
          "no-invalid-this": "off",
          "no-multiple-empty-lines": "error",
          "no-new-wrappers": "error",
          "no-null/no-null": "off",
          "no-shadow": [
            "off",
            {
              "hoist": "all"
            }
          ],
          "no-throw-literal": "error",
          "no-trailing-spaces": "error",
          "no-undef-init": "error",
          "no-underscore-dangle": [
            "off"
          ],
          "no-unsafe-finally": "error",
          "no-unused-expressions": "off",
          "no-unused-labels": "error",
          "no-var": "error",
          "object-shorthand": "off",
          "one-var": [
            "off",
            "never"
          ],
          "padding-line-between-statements": [
            "off",
            "error",
            {
              "blankLine": "always",
              "prev": "*",
              "next": "return"
            }
          ],
          "prefer-const": "error",
          "prefer-template": "off",
          "quote-props": "off",
          "radix": "error",
          "space-before-function-paren": [
            "error",
            {
              "anonymous": "never",
              "asyncArrow": "always",
              "named": "never"
            }
          ],
          "spaced-comment": [
            "error",
            "always",
            {
              "markers": [
                "/"
              ]
            }
          ],
          "use-isnan": "error",
          "valid-typeof": "off",
          "comma-dangle": [
            "error",
            {
              "arrays": "always-multiline",
              "objects": "always-multiline",
              "imports": "always-multiline",
              "exports": "always-multiline",
              "functions": "never"
            }
          ],
          "@typescript-eslint/typedef": "off",
          "react-hooks/rules-of-hooks": "error",
          "react-hooks/exhaustive-deps": "off",
          "react/jsx-indent": "off",
          "react/jsx-boolean-value": [
            "error",
            "always"
          ],
          "react/jsx-curly-spacing": [
            "error",
            {
              "when": "never"
            }
          ],
          "react/jsx-equals-spacing": [
            2,
            "never"
          ],
          "react/jsx-key": [
            2,
            {
              "checkFragmentShorthand": true
            }
          ],
          "react/no-string-refs": [
            2,
            {
              "noTemplateLiterals": true
            }
          ],
          "react/self-closing-comp": [
            "error",
            {
              "component": true,
              "html": false
            }
          ],
          "react/jsx-wrap-multilines": [
            2,
            {
              "declaration": "parens-new-line",
              "assignment": "parens-new-line",
              "return": "parens-new-line",
              "arrow": "parens-new-line",
              "condition": "ignore",
              "logical": "ignore",
              "prop": "ignore"
            }
          ],
          "react/no-this-in-sfc": "error",
          "react/no-unsafe": "error",
          "react/no-typos": "error",
          "react/display-name": "off",
          "react/prop-types": "off",
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
